int main () {
    FILE *fp;
    fp = fopen("FILENAMEW", "r");
    int line_Number = 0;
    char column[800];
    

    while (!feof (fp)){
        fgets (column, sizeof column, fp);
    fscanf(fp, "%[^;]", column);
    printf("%s\n", column);
        line_Number++;
    }
    fclose(fp);



    return (0);
}
